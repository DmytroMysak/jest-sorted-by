{
  "name": "@netly/jest-sorted-by",
  "version": "1.0.4",
  "description": "Sort array validation for jest",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "index.d.ts",
  "author": "Dmytro Mysak",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DmytroMysak/jest-sorted-by"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prebuild": "rimraf dist ",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint . --format stylish",
    "pretest": "npm run build",
    "test": "jest tests",
    "cz": "cz",
    "release": "release-it"
  },
  "devDependencies": {
    "@netly/eslint-config-base": "^1.0.0",
    "@netly/prettier-config": "^1.0.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "conventional-changelog": "^3.1.25",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./dist/cjs/index.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "prettier": "@netly/prettier-config",
  "engines": {
    "node": ">=16.13"
  },
  "keywords": [
    "jest",
    "sorted",
    "sorting",
    "sort",
    "toBeSortedBy",
    "extends"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
